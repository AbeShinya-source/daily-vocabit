APP_NAME="Daily Vocabit"
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=http://YOUR_DOMAIN_OR_IP
APP_TIMEZONE=Asia/Tokyo

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=error

# SQLite
DB_CONNECTION=sqlite
DB_DATABASE=/var/www/toeic-daily/backend/database/database.sqlite

# Session & Cache
SESSION_DRIVER=file
SESSION_LIFETIME=120
CACHE_DRIVER=file

# Sanctum
SANCTUM_STATEFUL_DOMAINS=YOUR_DOMAIN_OR_IP

# Google OAuth (必要に応じて設定)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# Gemini API
GEMINI_API_KEY=YOUR_GEMINI_API_KEY

# Mail (必要に応じて設定)
MAIL_MAILER=log
